/ Main jumbotron for a primary marketing message or call to action
.container
  .jumbotron.text-center
    %h1 gCamp
    %p Your life, organized
.container
  / Example row of columns
  .row
    - @columns.each do |title, contents|
      .col-md-4
        .panel.panel-primary
          .panel-heading
            %h3.panel-title
              = title
          .panel-body
            - if contents.length <= 1
              %p
                = contents[0]
            -else
              - contents.each do |content|
                %ul
                  %li
                    = content
/ /container
